<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:padding="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/back_img"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:padding="12dp"
            app:srcCompat="@drawable/white_back"
            app:tint="@color/black" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:fontFamily="@font/lato_bold"
            android:text="Hospital Tie-ups"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Insurance Tie-ups (Non GIPSA)"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/non_gipsa_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp" />

                    <Button
                        android:id="@+id/non_gipsa_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Insurance Tie-ups (GIPSA)"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/gipsa_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp" />

                    <Button
                        android:id="@+id/gipsa_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Government Sector"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/government_sector_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp" />

                    <Button
                        android:id="@+id/government_sector_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Public Sector"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/public_sector_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp" />

                    <Button
                        android:id="@+id/public_sector_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Private Sector"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/private_sector_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp"
                       />

                    <Button
                        android:id="@+id/private_sector_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:textSize="16dp"
                        android:fontFamily="@font/lato_medium"
                        android:textColor="@color/black"
                        android:text="Certifications"/>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/certifications_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="8dp" />

                    <Button
                        android:id="@+id/certifications_view_all_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato"
                        android:padding="12dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="16dp"
                        android:textSize="12dp"
                        android:text="View All"
                        android:textAllCaps="false"
                        app:cornerRadius="26dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/light_gray1"/>

                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>